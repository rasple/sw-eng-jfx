<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="407.0" prefWidth="411.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="controller.ProduktdatenController">
    <children>
        <Label fx:id="label" layoutX="22.0" layoutY="14.0" prefHeight="34.0" prefWidth="217.0" text="Produktdaten">
            <font>
                <Font size="22.0"/>
            </font>
        </Label>
        <Button fx:id="ok" layoutX="182.0" layoutY="366.0" mnemonicParsing="false" onMouseClicked="#onClickSave"
                prefHeight="34.0" prefWidth="102.0" text="OK"/>
        <Button fx:id="cancel" layoutX="292.0" layoutY="366.0" mnemonicParsing="false" onMouseClicked="#onClickAbort"
                prefHeight="34.0" prefWidth="95.0" text="Abbrechen"/>
        <TableView fx:id="table" layoutX="22.0" layoutY="59.0" prefHeight="301.0" prefWidth="364.0">
            <columns>
                <TableColumn fx:id="id" prefWidth="64.0" text="ID"/>
                <TableColumn fx:id="desc" prefWidth="95.0" text="Beschreibung"/>
                <TableColumn fx:id="ret" prefWidth="53.0" text="RET"/>
                <TableColumn fx:id="det" prefWidth="50.0" text="DET"/>
                <TableColumn fx:id="type" prefWidth="101.0" text="Typ"/>
            </columns>
        </TableView>
        <Button fx:id="add" layoutX="318.0" layoutY="31.0" mnemonicParsing="false" onMouseClicked="#onClickAdd"
                text="+"/>
        <Button fx:id="remove" layoutX="358.0" layoutY="31.0" mnemonicParsing="false" onMouseClicked="#onClickRemove"
                prefHeight="26.0" prefWidth="29.0" text="-"/>
    </children>
</AnchorPane>
